{
  "name": "aliff/chip-in",
  "description": "Laravel package for CHIP Collect payment gateway integration.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aliff Izzuddin",
      "email": "mrmoon23rd@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Aliff\\ChipIn\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aliff\\ChipIn\\ChipInServiceProvider"
      ],
      "aliases": {
        "ChipIn": "Aliff\\ChipIn\\Facades\\ChipIn"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
